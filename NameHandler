using System;

class NameHandler
{
    private string[] names = new string[5];

    public void GetNames()
    {
        Console.WriteLine("Enter 5 names:");
        for (int i = 0; i < 5; i++)
        {
            names[i] = Console.ReadLine();
        }
    }

    public void DisplayDuplicateCount()
    {
        int duplicateCount = 0;
        for (int i = 0; i < names.Length; i++)
        {
            for (int j = i + 1; j < names.Length; j++)
            {
                if (names[i] == names[j])
                {
                    duplicateCount++;
                    break;
                }
            }
        }
        Console.WriteLine($"Duplicate count: {duplicateCount}");
    }

    public void DisplayConsonantCount()
    {
        int consonantCount = 0;
        foreach (string name in names)
        {
            consonantCount += CountConsonants(name);
        }
        Console.WriteLine($"Consonant count: {consonantCount}");
    }

    private int CountConsonants(string name)
    {
        int count = 0;
        foreach (char c in name)
        {
            if ("bcdfghjklmnpqrstvwxyzBCDFGHJKLMNPQRSTVWXYZ".Contains(c))
            {
                count++;
            }
        }
        return count;
    }

    public void DisplayReversedNames()
    {
        Console.WriteLine("Reversed names:");
        foreach (string name in names)
        {
            string reversedName = ReverseString(name);
            Console.WriteLine(reversedName);
        }
    }

    private string ReverseString(string input)
    {
        char[] charArray = input.ToCharArray();
        for (int i = 0, j = charArray.Length - 1; i < j; i++, j--)
        {
            char temp = charArray[i];
            charArray[i] = charArray[j];
            charArray[j] = temp;
        }
        return new string(charArray);
    }

    public void DisplaySortedReversedNames()
    {
        string[] reversedNames = new string[names.Length];
        for (int i = 0; i < names.Length; i++)
        {
            reversedNames[i] = ReverseString(names[i]);
        }

        Array.Sort(reversedNames);

        Console.WriteLine("Sorted reversed names:");
        foreach (string name in reversedNames)
        {
            Console.WriteLine(name);
        }
    }
}
